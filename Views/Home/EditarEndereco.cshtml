@model vacinacao.DTO.EnderecoDTO

<h3>Editar endereço</h3>
<hr>
<form class="form-horizontal" asp-controller="Enderecos" asp-action="Atualizar" method="POST">
    <input type="hidden" asp-for="Id"> 

    <label asp-for="Cep">CEP</label>
    <input type="number" asp-for="Cep" class="form-control" placeholder="00000000">
    <span asp-validation-for="Cep"></span><br>
    
    <label asp-for="Logradouro">Logradouro</label>
    <input type="text" asp-for="Logradouro" class="form-control" placeholder="Rua Exemplo">
    <span asp-validation-for="Logradouro"></span><br>

    <label asp-for="Numero">Número</label>
    <input type="number" asp-for="Numero" class="form-control" placeholder="111">
    <span asp-validation-for="Numero"></span><br>

    <label asp-for="Complemento">Complemento</label>
    <input type="text" asp-for="Complemento" class="form-control" placeholder="Complemento">
    <span asp-validation-for="Complemento"></span><br>

    <label asp-for="Municipio">Município</label>
    <input type="text" asp-for="Municipio" class="form-control" placeholder="Recife">
    <span asp-validation-for="Municipio"></span><br>

    <label asp-for="Estado">Estado</label>
    <input type="text" asp-for="Estado" class="form-control" placeholder="PE">
    <span asp-validation-for="Estado"></span><br>

    <button class="btn btn-success">Atualizar</button>
</form>